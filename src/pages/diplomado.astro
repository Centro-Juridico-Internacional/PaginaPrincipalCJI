---
import Layout from '@layouts/Layout.astro';

const title = '¿CÓMO IMPLEMENTAR Y AUDITAR LOS REQUISITOS DEL SG-SST EN SU ORGANIZACIÓN?';
---

<Layout imgLayout="" title="Diplomado">
	<main class="flex flex-col items-center justify-center bg-gray-50 pt-10">
		<h1 class="w-[50%] flex-wrap text-center text-4xl font-semibold">
			{title}
		</h1>
		<img src="/diplomado/diplomado.webp" alt="diplomado imagen" class="mt-10" />
	</main>
</Layout>
